---
layout: null
permalink: /feed/feed.xml
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ metadata.title }}</title>
  <subtitle>{{ metadata.description }}</subtitle>
  <link href="{{ metadata.url }}/" rel="alternate"/>
  <link href="{{ metadata.url }}/feed/feed.xml" rel="self"/>
  <id>{{ metadata.url }}/</id>
  <updated>{{ collections.posts[0].date | dateToRfc3339 }}</updated>
  <author>
    <name>{{ metadata.author.name }}</name>
  </author>
{%- for post in collections.posts | limit(10) %}
  <entry>
    <title>{{ post.data.title }}</title>
    <link href="{{ metadata.url }}{{ post.url }}" rel="alternate"/>
    <link href="{{ metadata.url }}{{ post.url }}" rel="alternate" type="text/html"/>
    <id>{{ metadata.url }}{{ post.url }}</id>
    <published>{{ post.data.date | dateToRfc3339 }}</published>
    <updated>{{ post.data.date | dateToRfc3339 }}</updated>
    <summary>{{ post.data.description }}</summary>
    <content type="html"><![CDATA[
      {{ post.templateContent | safe }}
    ]]></content>
    <author>
      <name>{{ metadata.author.name }}</name>
    </author>
  </entry>
{%- endfor %}
</feed>
